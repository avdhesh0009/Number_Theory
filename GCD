#include<bits/stdc++.h>
using namespace std;
// Time complexity of GCD O(log(N))
// 1<=a,b<=10^9
int gcd(int a,int b){
    if(b==0) return a;
    return gcd(b,a%b);
}
void solve(){
   int a,b;
   cin>>a>>b;
   cout<<gcd(a,b)<<endl;
}
int main(){
    int t=1;
    // cin>>t;
    for(int i=1;i<=t;i++){
        solve();
    }
   return 0;
}
